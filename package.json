{
  "name": "test-area",
  "license": "MIT",
  "author": "Glenn <glenn@not-tech.support> (https://not-tech.support)",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/promise"
    },
    {
      "type": "patreon",
      "url": "https://patreon.com/promises"
    },
    "http://paypal.me/nottechsupport"
  ],
  "main": "build",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "docker": "npm run docker:build && npm run docker:up",
    "docker:build": "docker-compose --project-directory . build",
    "docker:down": "docker-compose --project-directory . down",
    "docker:logs": "docker-compose --project-directory . logs --tail=500 -f",
    "docker:start": "npm run docker:up",
    "docker:stop": "npm run docker:down",
    "docker:up": "docker-compose --project-directory . up -d",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node .",
    "test": "jest"
  },
  "dependencies": {
    "@sapphire/type": "2.4.0",
    "@typegoose/typegoose": "11.3.0",
    "dedent": "0.7.0",
    "discord.js": "14.11.0",
    "dotenv": "16.1.4",
    "friendly-words": "1.2.2",
    "match-sorter": "6.3.1",
    "mongoose": "7.3.1",
    "winston": "3.9.0",
    "winston-daily-rotate-file": "4.7.1"
  },
  "devDependencies": {
    "@tsconfig/node18-strictest": "1.0.0",
    "@types/dedent": "0.7.0",
    "@types/friendly-words": "1.2.0",
    "@types/jest": "29.5.2",
    "@types/node": "18.16.18",
    "eslint": "8.43.0",
    "eslint-config-promise": "github:promise/eslint-config",
    "jest": "29.5.0",
    "ts-jest": "29.1.0",
    "typescript": "4.9.5"
  },
  "optionalDependencies": {
    "bufferutil": "4.0.7",
    "erlpack": "github:discord/erlpack",
    "utf-8-validate": "5.0.10",
    "zlib-sync": "0.1.8"
  },
  "eslintConfig": {
    "extends": "promise"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ]
  }
}
